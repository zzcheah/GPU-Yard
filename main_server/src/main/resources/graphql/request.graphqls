type Request {
    id: String!
    name: String!
    status: String!
    createdAt: String!
    title: String!
    image: String!
    user: User!
    encodedParam: String!
    assignedTo: String
    inputFiles: [String]
    outputFiles: [String]
}

# Root
type Query {
    #    findAllAuthors: [Request]!
    countRequests: Int!
    myRequests: [Request]!
    allRequests: [Request]!
}

# Root
type Mutation {
    createRequest(input: CreateRequestInput): Request!
}
